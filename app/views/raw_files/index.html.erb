<h1>Listing raw_files</h1>

<table>
  <tr>
    <th>Path</th>
    <th>Tags</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @raw_files.each do |raw_file| %>
    <tr>
      <td><%= raw_file.path %></td>
      <td><%= raw_file.tags %></td>
      <td><%= link_to 'Show', raw_file %></td>
      <td><%= link_to 'Edit', edit_raw_file_path(raw_file) %></td>
      <td><%= link_to 'Destroy', raw_file, confirm: 'Are you sure?', method: :delete %></td>
    </tr>
  <% end %>
</table>

<br/>

<%= link_to 'New Raw file', new_raw_file_path %>
